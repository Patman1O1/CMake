@PACKAGE_INIT@

# Only import if not already defined
if(NOT TARGET rclone_sync::rclone_sync)
    if(NOT EXISTS "${CMAKE_CURRENT_LIST_DIR}/rclone_sync_targets.cmake")
        set(${CMAKE_FIND_PACKAGE_NAME}_NOT_FOUND_MESSAGE "rclone_sync libraries were requested but not found")
        set(${CMAKE_FIND_PACKAGE_NAME}_FOUND OFF)
        return()
    endif()

    include("${CMAKE_CURRENT_LIST_DIR}/rclone_sync_targets.cmake")
    check_required_components(rclone_sync)
    message("-- Found rclone_sync (version ${${CMAKE_FIND_PACKAGE_NAME}_VERSION})")
endif()
